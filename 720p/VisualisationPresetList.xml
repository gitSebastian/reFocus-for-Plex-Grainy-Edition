<window id="122">
	<defaultcontrol>2</defaultcontrol>
	
	<coordinates>
		<system>1</system>
		<posx>340</posx>
		<posy>100</posy>
	</coordinates>
	
	<include>dialogOpen</include>
		
	<controls>
		<control type="image">
			<width>600</width>
			<height>475</height>
			<include>dialogWindow</include>
		</control>
		
		<control type="label" id="3">
			<description>heading label</description>
			<width>532</width>
			<scroll>true</scroll>
			<include>dialogHeading</include>
		</control>
		
		<control type="image">
			<posx>19</posx>
			<posy>75</posy>
			<width>563</width>
			<include>dialogSeparator</include>
		</control>
		
		<control type="label" id="4">
			<description>No presets Label</description>
			<posx>19</posx>
			<posy>220</posy>
			<width>563</width>
			<align>center</align>
			<label>13389</label>
			<textcolor>Color4</textcolor>
			<font>normalLow</font>
		</control>
		
		<control type="list" id="2">
			<posx>19</posx>
			<posy>75</posy>
			<width>563</width>
			<height>320</height>
			<onup>2</onup>
			<onleft>-</onleft>
			<onright>-</onright>
			<ondown>2</ondown>
			<scrolltime>200</scrolltime>

			<itemlayout height="40">
				<control type="image">
					<width>600</width>
					<height>40</height>
					<texture border="3">dialogs/list-nofocus.png</texture>
				</control>
				<control type="label">
					<posx>15</posx>
					<width>563</width>
					<height>35</height>
					<font>mediumLow</font>
					<align>left</align>
					<aligny>center</aligny>
					<info>ListItem.Label</info>
					<textcolor>Color4</textcolor>
					<focusedcolor>Color1</focusedcolor>
				</control>
			</itemlayout>
			<focusedlayout height="40">
				<control type="image">
					<width>600</width>
					<height>40</height>
					<texture border="3">dialogs/list-focus.png</texture>
				</control>
				<control type="label">
					<posx>15</posx>
					<width>563</width>
					<height>35</height>
					<font>mediumLow</font>
					<align>left</align>
					<aligny>center</aligny>
					<info>ListItem.Label</info>
					<textcolor>Color4</textcolor>
					<focusedcolor>Color1</focusedcolor>
				</control>
			</focusedlayout>
		</control>

		<control type="label">
			<posx>34</posx>
			<posy>405</posy>
			<width>534</width>
			<height>30</height>
			<align>center</align>
			<aligny>center</aligny>
			<font>medium</font>
			<label>$INFO[Container(2).CurrentPage]/$INFO[Container(2).NumPages]</label>
			<visible>!Control.IsVisible(451) + !Control.IsVisible(4)</visible>
			<textcolor>Color4</textcolor>
			<shadowcolor>fff4f4f4</shadowcolor>
		</control>
	</controls>
</window>