				<colordiffuse>Color7</colordiffuse>
				<aspectratio align="center">keep</aspectratio>
				<visible>!Container.Content(seasons)</visible>
			</control>
			<control type="image">
				<width>96</width>
				<height>34</height>
				<texture>$INFO[Listitem.Property(mediaTag-videoResolution),flags/resolution/,.png]</texture>
				<aspectratio align="center">keep</aspectratio>
			</control>
